{
  "name": "workix",
  "version": "1.0.0",
  "description": "> Empowering non-technical users to create, run, and monitor AI-driven workflows without writing code.",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "npm run generate:prisma-types:monolith",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "vitest run --config apps/api-monolith/src/app/integrations/vitest.e2e.config.ts",
    "test:e2e:watch": "vitest --config apps/api-monolith/src/app/integrations/vitest.e2e.config.ts",
    "test:auth:e2e": "vitest run apps-e2e/src/auth/auth.spec.ts --config apps-e2e/vitest.config.auth.ts",
    "test:auth:e2e:watch": "vitest apps-e2e/src/auth/auth.spec.ts --config apps-e2e/vitest.config.auth.ts",
    "test:auth:e2e:setup": "bash scripts/setup-auth-e2e.sh",
    "test:auth:e2e:full": "bash scripts/start-test-services.sh && bash scripts/run-e2e-tests.sh",
    "test:auth:e2e:services": "bash scripts/start-test-services.sh",
    "test:auth:e2e:run": "bash scripts/run-e2e-tests.sh",
    "test:auth:e2e:all": "bash scripts/setup-auth-e2e-db.sh && bash scripts/start-auth-service-for-tests.sh && sleep 3 && vitest run apps-e2e/src/auth/api-auth-endpoints.e2e.spec.ts --config apps-e2e/vitest.config.auth.ts",
    "test:auth:e2e:all:watch": "bash scripts/setup-auth-e2e-db.sh && bash scripts/start-auth-service-for-tests.sh && sleep 3 && vitest apps-e2e/src/auth/api-auth-endpoints.e2e.spec.ts --config apps-e2e/vitest.config.auth.ts",
    "test:auth:e2e:security": "bash scripts/setup-auth-e2e-db.sh && bash scripts/start-auth-service-for-tests.sh && sleep 3 && vitest run apps-e2e/src/auth/security-injection.spec.ts --config apps-e2e/vitest.config.auth.ts",
    "test:auth:e2e:security:watch": "bash scripts/setup-auth-e2e-db.sh && bash scripts/start-auth-service-for-tests.sh && sleep 3 && vitest apps-e2e/src/auth/security-injection.spec.ts --config apps-e2e/vitest.config.auth.ts",
    "test:auth:e2e:db": "bash scripts/setup-auth-e2e-db.sh",
    "test:auth:e2e:service": "bash scripts/start-auth-service-for-tests.sh",
    "test:auth:integration": "nx test api-auth --testPathPattern=integration",
    "test:auth:integration:setup": "bash scripts/setup-test-auth-db.sh",
    "test:auth:with-db": "bash scripts/test-auth-with-db.sh all",
    "test:auth:with-db:security": "bash scripts/test-auth-with-db.sh security",
    "test:auth:with-db:integration": "bash scripts/test-auth-with-db.sh integration",
    "api:serve": "nx serve api-gateway",
    "api:build": "nx build api-gateway",
    "api:start": "node dist/apps/api-gateway/main.js",
    "api:gateway": "nx serve api-gateway",
    "api:monolith": "nx serve api-monolith",
    "api:auth": "nx serve api-auth",
    "api:auth:dev": "docker-compose up -d postgres-auth && sleep 3 && cd apps/api-auth && DATABASE_URL_AUTH=postgresql://postgres:postgres@localhost:5200/workix_auth npx prisma generate --schema=./prisma/schema.prisma && DATABASE_URL_AUTH=postgresql://postgres:postgres@localhost:5200/workix_auth npx prisma db push --skip-generate --schema=./prisma/schema.prisma && cd ../.. && NODE_ENV=development API_AUTH_PORT=7200 JWT_SECRET=dev-jwt-secret-minimum-32-characters-long-for-development-only DATABASE_URL_AUTH=postgresql://postgres:postgres@localhost:5200/workix_auth SERVICE_KEY=dev-service-key-minimum-32-characters-long-for-development-only nx serve api-auth",
    "api:auth:build": "nx build api-auth",
    "api:auth:start": "node dist/apps/api-auth/main.js",
    "api:auth:prod": "docker-compose up -d postgres-auth && sleep 3 && cd apps/api-auth && npx prisma generate --schema=./prisma/schema.prisma && npx prisma migrate deploy --schema=./prisma/schema.prisma && cd ../.. && npm run api:auth:build && npm run api:auth:start",
    "api:auth:start-script": "bash scripts/start-auth-service.sh dev",
    "api:auth:start-script:prod": "bash scripts/start-auth-service.sh prod",
    "app:web": "nx serve app-web",
    "app:admin": "nx serve app-admin",
    "mcp:info": "echo 'MCP Server информация' && node -e \"console.log(require('./package.json').devDependencies)\"",
    "mcp:list": "npm ls | grep mcp",
    "mcp:build": "cd apps/mcp-server && npm run build",
    "mcp:dev": "cd apps/mcp-server && npm run dev",
    "mcp:start": "cd apps/mcp-server && npm start",
    "mcp:status": "bash scripts/mcp-servers.sh status",
    "mcp:start-all": "bash scripts/mcp-servers.sh start",
    "mcp:stop-all": "bash scripts/mcp-servers.sh stop",
    "mcp:restart-all": "bash scripts/mcp-servers.sh restart",
    "mcp:pull-models": "bash scripts/mcp-servers.sh pull",
    "mcp:config": "bash scripts/mcp-servers.sh config",
    "dev": "concurrently \"npm run api:gateway\" \"npm run mcp:start-all\" --names \"API-Gateway,MCP-ALL\" --prefix \"[{name}]\" --colors",
    "dev:full": "concurrently \"npm run api:gateway\" \"npm run mcp:start-all\" \"npm run app:admin\" \"npm run app:web\" --names \"Gateway,MCP,Admin,Web\" --prefix \"[{name}]\" --colors",
    "dev:mcp-only": "npm run mcp:dev",
    "setup:cursor": "bash scripts/setup-cursor-mcp.sh",
    "auto:cycle": "bash scripts/auto-dev-cycle.sh",
    "auto:test": "npm run test:run",
    "auto:db:setup": "docker-compose up -d",
    "auto:db:down": "docker-compose down",
    "auto:db:reset": "docker-compose down -v && docker-compose up -d",
    "db:dev": "docker-compose up -d postgres-monolith postgres-auth postgres-notifications redis",
    "db:dev:down": "docker-compose stop postgres-monolith postgres-auth postgres-notifications redis",
    "db:dev:logs": "docker-compose logs -f postgres-monolith postgres-auth postgres-notifications redis",
    "services:dev": "npm run db:dev && sleep 3 && concurrently \"npm run api:auth:dev\" \"npm run api:notifications:dev\" --names \"Auth,Notifications\" --prefix \"[{name}]\" --colors",
    "api:notifications:dev": "NODE_ENV=development API_NOTIFICATIONS_PORT=7201 DATABASE_URL_NOTIFICATIONS=postgresql://postgres:postgres@localhost:5201/workix_notifications REDIS_HOST=localhost REDIS_PORT=5900 nx serve api-notifications",
    "api:notifications:build": "nx build api-notifications",
    "api:notifications:start": "node dist/apps/api-notifications/main.js",
    "services:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "services:prod:down": "docker-compose -f docker-compose.prod.yml down",
    "services:prod:logs": "docker-compose -f docker-compose.prod.yml logs -f",
    "storybook": "nx storybook shared-frontend-ui",
    "build-storybook": "nx build-storybook shared-frontend-ui",
    "check:swagger": "bash scripts/check-swagger.sh",
    "check:swagger:monolith": "bash scripts/check-swagger.sh http://localhost:7000/api/docs-json",
    "generate:api-types": "bash scripts/generate-api-types.sh",
    "generate:api-types:monolith": "bash scripts/generate-api-types.sh http://localhost:7000/api/docs-json libs/shared/frontend/core/src/types/api-monolith.types.ts",
    "generate:api-types:gateway": "bash scripts/generate-api-types.sh http://localhost:7100/docs-json libs/shared/frontend/core/src/types/api-gateway.types.ts",
    "generate:api-types:all": "bash scripts/generate-all-api-types.sh",
    "generate:api-types:auto": "bash scripts/generate-types-with-server.sh",
    "generate:prisma-types": "bash scripts/generate-prisma-types.sh",
    "gateway:seed": "cd apps/api-gateway && DATABASE_URL_GATEWAY=postgresql://postgres:postgres@localhost:5000/workix_gateway npx tsx prisma/seed.ts",
    "generate:prisma-types:monolith": "bash scripts/generate-prisma-types.sh apps/api-monolith/prisma/schema.prisma",
    "remove-unused": "node scripts/remove-unused-simple.js",
    "remove-unused:check": "node scripts/remove-unused-simple.js --dry-run",
    "add-return-types": "npx ts-node scripts/add-return-types-improved.ts",
    "add-return-types:check": "npx ts-node scripts/add-return-types-improved.ts --dry-run",
    "add-types": "npx ts-node scripts/add-types-complete.ts",
    "add-types:check": "npx ts-node scripts/add-types-complete.ts --dry-run",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,html,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,html,css,scss}\"",
    "lint": "npx eslint . --fix",
    "lint:check": "npx eslint .",
    "lint:fix": "npx eslint . --fix --ext .ts,.tsx",
    "fix:types": "npm run add-types",
    "fix:unused": "npm run remove-unused",
    "fix:all": "npm run format && npm run lint:fix && npm run fix:types && npm run fix:unused",
    "fix:auto": "npm run format && npm run lint:fix"
  },
