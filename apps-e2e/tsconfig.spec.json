{
  "extends": [
    "./tsconfig.json",
    "../tsconfig.spec.base.json"
  ],
  "compilerOptions": {
    "outDir": "../dist/out-tsc",
    "module": "commonjs",
    "types": [
      "vitest/globals",
      "node"
    ],
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.spec.ts"
  ]
}
