{
  "extends": [
    "../../../tsconfig.base.json",
    "../../../tsconfig.spec.base.json"
  ],
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.test.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
